export ORACLE_HOME=/opt/db02/app/oracle/product/11.1.0/db_1

# Database to be configured
export DATABASE=ivtfin
export SYS_PASSWD=manager
export SYSTEM_PASSWD=manager
export LOGDIR=/tmp
export PATH=$ORACLE_HOME/bin:$PATH

# The following needs to be valid Path in Database Server Machine
export ORADATA_HOME=/opt/db02/app/oracle/oradata

# TableSpace directory of Different Schema in DB Server.
export SI_TS_ROOT=${ORADATA_HOME}/${DATABASE}
export CEIDB_TS_ROOT=${ORADATA_HOME}/${DATABASE}
export BPCDB_TS_ROOT=${ORADATA_HOME}/${DATABASE}
export BPCOBS_TS_ROOT=${ORADATA_HOME}/${DATABASE}
export BSPACE_TS_ROOT=${ORADATA_HOME}/${DATABASE}
