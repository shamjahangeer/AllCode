PROMPT  Expand CCE_ITEM_CATEGORY.ITEM_CAT_ICON_IMAGE_NAME to VARCHAR2(256)
DECLARE 
  l_table_name CONSTANT VARCHAR2(30) := 'CCE_ITEM_CATEGORY';
  l_column_name CONSTANT VARCHAR2(30) := 'ITEM_CAT_ICON_IMAGE_NAME';
  l_data_type CONSTANT VARCHAR2(128) := ' VARCHAR2(256)';
  l_length_should_be CONSTANT INTEGER := 256;
  l_data_length INTEGER;  
BEGIN
  SELECT data_length
  INTO   l_data_length
  FROM   user_tab_columns
  WHERE  table_name = l_table_name
  AND    column_name = l_column_name;
  
  IF l_data_length < l_length_should_be THEN  
     EXECUTE IMMEDIATE 'ALTER TABLE '||l_table_name||' MODIFY '||l_column_name||l_data_type;
  END IF;
  
  EXCEPTION
    WHEN NO_DATA_FOUND THEN NULL;
END;
/

